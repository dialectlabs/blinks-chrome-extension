    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "gulp": "^5.0.0",
    "gulp-zip": "^6.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
